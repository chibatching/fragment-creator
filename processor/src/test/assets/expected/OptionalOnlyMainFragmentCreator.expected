package com.sys1yagi.fragmentcreator.fragment;

import android.os.Bundle;
import com.sys1yagi.fragmentcreator.FragmentCreator;
import java.lang.String;

public final class MainFragmentCreator extends FragmentCreator {


    public static void read(MainFragment fragment) {
        Bundle args = fragment.getArguments();
        fragment.keyword = args.getString("keyword");
        fragment.userId = args.getString("userId");
        checkRequired(fragment);
    }

    public static void checkRequired(MainFragment fragment) {
    }

      public static final class Builder {
        String keyword;

        String userId;

        private Builder() {
        }

        public static Builder newInstance() {
          Builder builder = new Builder();
          return builder;
        }

        public Builder setKeyword(String keyword) {
          this.keyword = keyword;
          return this;
        }

        public Builder setUserId(String userId) {
          this.userId = userId;
          return this;
        }

        public MainFragment build() {
          MainFragment fragment = new MainFragment();
          Bundle args = new Bundle();
          args.putString("keyword", keyword);
          args.putString("userId", userId);
          fragment.setArguments(args);
          return fragment;
        }
      }
}
